    "version": "0.2.0",
    "configurations": [
      {
        "name": "cds serve sanbox",
        "request": "launch",
        "type": "node",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "cds",
        "args": [
          "serve",
          "--with-mocks",
          "--in-memory?",
          "--profile",
          "sandbox"
        ],
        "skipFiles": [
          "<node_internals>/**"
        ]
      },
      {
        "name": "cds serve live data",
        "request": "launch",
        "type": "node",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "cds",
        "args": [
          "serve",
          "--with-mocks",
          "--in-memory?",
          "--profile",
          "live"
        ],
        "skipFiles": [
          "<node_internals>/**"
        ]
      }
    ]
  }